{
  "name": "GCal Unlimited Weeks",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Display as many weeks in Google Calendar as your screen can handle",
  "homepage_url": "https://github.com/tomviner/gcal-unlimited-weeks",

  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://calendar.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://calendar.google.com/*"
      ],
      "js": [
        "src/inject/jquery.min.js",
        "src/inject/inject.js"
      ]
    }
  ]
}